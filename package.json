    "start": "probot run ./index.js",
    "lint": "standard",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "types": "tsc --p jsconfig.json"
  },
  "dependencies": {
    "@types/node": "^12.0.2",
    "eslint": "^5.16.0",
    "eslint-config-kasko-base": "^1.0.0",
    "probot": "^7.2.0",
    "source-map": "^0.7.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
